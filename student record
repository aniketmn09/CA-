class Student:
    def __init__(self,s_id,s_name,s_coursesode):
        self.s_id = s_id
        self.s_name = s_name
        self.s_coursecode = s_coursecode
        
    def showrecord(self):
        print(self.s_id,end='\t\t')
        print(self.s_name,end='\t\t')
        print(self.s_coursecode)
        
class Dataoperation:
    def quicksort(self,s_id):
        if len(s_id) == 1 or len(s_id) == 0:
            return s_id
        else:
            pivot = s_id[0]
            i = 0

        
record = []    
n = int(input('Enter no of students'))
for i in range (n):
    s_id = input("Enter Student Id")
    while (not len(s_id) == 8):
        print("Please Enter 8 digit ID")
        s_id = input("Enter Student Id")
    s_name = input("Enter Student Name ")
    s_coursecode = input("Enter Student Coursecode")
    while (not len(s_coursecode) == 7):
        print("Please Enter 7 digit Coursecode")
        s_coursecode = input("Enter Student coursecode")
    record.append(Student(s_id,s_name,s_coursecode))

for i in range(n):
    record[i].showrecord()
